@inherits LayoutComponentBase
@inject States States

@if (States.IsLoging)
{
    <div class="main">
        <div class="content">
            @Body
        </div>
    </div>
}
else
{
    <Layout>
         <Main>
            @Body
        </Main> 
    </Layout>

}

@code{

    [Parameter]
    public EventCallback Changed { get; set; }

}